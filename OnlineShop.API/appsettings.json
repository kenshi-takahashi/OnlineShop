{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "File": {
    "LogLevel": {
      "Default": "Information"
    },
    "Path": "Logs/app.log",
    "RollingInterval": "Day"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_server;Database=your_database;User=your_user;Password=your_password;Trusted_Connection=True;TrustServerCertificate=True;"
  }
}